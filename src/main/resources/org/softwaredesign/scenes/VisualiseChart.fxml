   <children>
      <AreaChart fx:id="chartDisplay" layoutX="50.0">
        <xAxis>
          <NumberAxis fx:id="xAx" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAx" side="LEFT" />
        </yAxis>
      </AreaChart>
   </children>
</AnchorPane>
